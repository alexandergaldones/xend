<?xml version="1.0" encoding="utf-8"?>
<soap:Envelope xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">
  <soap:Header>
    <AuthHeader xmlns="https://www.xend.com.ph/api/">
      <UserToken>{{ Settings::DEVELOPER_ID }}</UserToken>
    </AuthHeader>
  </soap:Header>
  <soap:Body>
    <Calculate xmlns="https://www.xend.com.ph/api/">
      <ServiceTypeValue>MetroManilaExpress</ServiceTypeValue>
      <ShipmentTypeValue>Parcel</ShipmentTypeValue>
      <DestinationValue>Manila</DestinationValue>
      <Weight>{{ weight }}</Weight>
    <!--
      <DimensionL>Not required</DimensionL>
      <DimensionW>Not required</DimensionW>
      <DimensionH>Not required</DimensionH>
      <DeclaredValue>Not required</DeclaredValue>
      <AddInsurance>Not required</AddInsurance>
    -->
    </Calculate>
  </soap:Body>
</soap:Envelope>